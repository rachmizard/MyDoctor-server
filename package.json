{
	"name": "mydoctor-server",
	"version": "1.0.0",
	"description": "mydoctor-server",
	"main": "src/index.js",
	"scripts": {
		"start": "set NODE_ENV=production&& nodemon --exec babel-node -r dotenv/config ./src dotenv_config_path=.env",
		"dev": "set NODE_ENV=development&& nodemon --exec babel-node -r dotenv/config ./src dotenv_config_path=.env.development",
		"clean": "rm -rf build && mkdir build && cp .env ./build",
		"test": "echo \"Error: no test specified\" && exit 1",
		"build-babel": "babel -d ./build ./src -s",
		"build": "npm run clean && npm run build-babel"
	},
	"author": "rachmizard",
	"license": "ISC",
	"devDependencies": {
		"@babel/cli": "^7.14.8",
		"@babel/core": "^7.15.0",
		"@babel/node": "^7.14.9",
		"@babel/plugin-transform-runtime": "^7.15.0",
		"@babel/polyfill": "^7.12.1",
		"@babel/preset-env": "^7.15.0",
		"@babel/runtime": "^7.15.3",
		"eslint": "^7.32.0",
		"nodemon": "^2.0.12"
	},
	"dependencies": {
		"apollo-server": "^3.1.2",
		"apollo-server-core": "^3.1.2",
		"apollo-server-express": "^3.1.2",
		"bcrypt": "^5.0.1",
		"dotenv": "^10.0.0",
		"express": "^4.17.1",
		"firebase": "^8.10.0",
		"graphql": "^15.5.1",
		"jsonwebtoken": "^8.5.1",
		"moment": "^2.29.1"
	}
}
